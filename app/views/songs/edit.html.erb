<h1>Editing Song #<%= @song.id %></h1>

<form action="/songs/<%= @song.id %>/update">
  <div>
    <label for="name">name</label><br />
    <input id="name" name="name" value="<%= @song.name  %>" type="text" />
  </div>
  <div>
    <label for="image">image</label><br />
    <input id="image" name="image" value="<%= @song.image  %>" type="text" />
  </div>
  <div>
    <input type="submit">
  </div>
</form>


