<h1>Artist #<%= @artist.id %></h1>

<div>
  <strong>Name</strong>:<br />
  <p><%= @artist.name %></p>
</div>
<div>
  <strong>Image</strong>:<br />
  <img src="<%= @artist.image %>">
</div>
<div>
  <strong>songs</strong>:<br />
  <ul>
    <% @songs.each do |song| %>
    <li><%= song.name %></li>
    <% end %>
  </ul>
</div>

<a href="/artists/<%= @artist.id %>/edit">Edit</a> |
<a href="/artists/<%= @artist.id %>/destroy">Destroy</a>
